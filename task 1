control statements
marks=int(input("enter your grade"))

if(marks>=90):
  grade="A"
elif(marks >= 80 and marks <90):
  grade="B"

elif(marks >= 70 and marks < 80):
  grade="c"
else:
  grade="D"
print("grade of the students:",grade)



light="green"
if( light=="red"):
  print("stop")
elif(light=="green"):
  print("go")
else:
   print ("light is broken")



age = 95
if(age>=18):
  if(age>=80):
    print("can not drive")
  else:
    print("can drive")
else:
    print("can not drive")


